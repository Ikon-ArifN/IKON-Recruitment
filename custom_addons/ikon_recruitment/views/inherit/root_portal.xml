<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <template id="ikon_root_portal" name="Root Portal Ikon" inherit_id="website.homepage">
        <xpath expr="//t[@t-call='website.layout']" position="replace">
            <t t-call="website.layout">
                <t t-set="pageName" t-value="'homepage'" />
                <div id="wrap" class="oe_structure oe_empty">
                    <section class="s_banner parallax s_parallax_is_fixed pt96 pb96 o_colored_level"
                        data-scroll-background-ratio="1" data-snippet="s_banner" data-name="Banner"
                        style="background-image: none;">
                        <span class="s_parallax_bg oe_img_bg o_bg_img_center"
                            style="background-image: url(&quot;/web/image/983-bbb17404/1.png&quot;); background-position: 50% 0px;"
                            data-original-id="940" data-original-src="/web/image/940-b6b7c0e5/1.png"
                            data-mimetype="image/png" data-resize-width="1024" />
                        <div class="container">
                            <div class="row s_nb_column_fixed">
                                <div
                                    class="col-lg-6 jumbotron rounded o_cc o_cc1 pt32 pb32 o_colored_level"
                                    data-name="Box" style="">
                                    <h1>
                                        <font style="font-size: 62px;">Ikonsultan Inovatama</font>
                                        <br />
                                    </h1>
                                    <p class="lead">Since 2002, IKON has built a strong reputation
                                        through its consultants by their experience and knowledge in
                                        the
                                        financial industry, project management, and information
                                        technology..</p>
                                    <a class="mb-2 btn btn-primary"
                                        href="https://www.ikonsultan.com/"
                                        data-bs-original-title="" title="">Contact Us</a>
                                </div>
                            </div>
                        </div>
                    </section>
                </div>
            </t>
        </xpath>
    </template>

</odoo>
